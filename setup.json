{
    "name": "aiida-gaussian-datatypes",
    "author": "Tiziano MÃ¼ller",
    "author_email": "tiziano.mueller@chem.uzh.ch",
    "description": "AiiDA data plugin to manage gaussian datatypes (basis sets and pseudopotentials) as first-class citizens",
    "url": "https://github.com/dev-zero/aiida-gaussian-datatypes",
    "license": "MIT License",
    "classifiers": [
        "Programming Language :: Python",
        "License :: OSI Approved :: MIT License"
    ],
    "version": "0.1.0",
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida >= 0.9"
    ],
    "entry_points": {
        "aiida.data": [
            "gaussian_datatype.basisset = aiida_gaussian_datatypes.data.basisset:BasisSet",
            "gaussian_datatype.pseudopotential = aiida_gaussian_datatypes.data.pseudopotential:PseudoPotential"
        ],
        "console_scripts": [
            "aiida-gaussian-datatypes-basisset = aiida_template.console_scripts.data_cli:cli"
        ]
    },
    "scripts": ["examples/cli.py"]
}
